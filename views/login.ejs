  <body style="font-family: Raleway; background-color: #d7e0ef;">

      <div class="limiter">
        <div class="container-login100">
          <div class="wrap-login100">
            <form action="/login" method="POST">
              <span class="login100-form-title p-b-48">
                <img src="../images/mainlogo_b.png" alt="" style="width: 250px">
              </span>
              <span class="login100-form-title p-b-26">
                Log in
              </span>
              <div class="wrap-input100 validate-input" data-validate = "Valid email is: a@b.c">
                <input class="input100" type="email" name="username" placeholder=" Email" required>
                <span class="focus-input100"></span>
              </div>
              <div class="wrap-input100 validate-input" data-validate="Enter password">
                <span class="btn-show-pass">
                  <i class="zmdi zmdi-eye"></i>
                </span>
                <input class="input100" type="password" type="password" name="password" placeholder=" Password" required>
                <span class="focus-input100"></span>
              </div>
              <div class="container-login100-form-btn">
                <div class="wrap-login100-form-btn">
                  <div class="login100-form-bgbtn"></div>
                  <button class="login100-form-btn" type="submit">
                    Login
                  </button>
                </form>
                </div>
              </div>
              <div class="text-center p-t-70">
                <span class="txt1">
                  Don't have an account?
                </span>
                <form action="/signbutton" method="POST">
                   <button class="txt2">Sign up</button>
                  </form>

              </div>

          </div>
        </div>
      </div>




